{
  "name": "translate-projects-react",
  "version": "0.0.3",
  "description": "Internationalize and manage your website easily",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./utils": "./dist/utils/index.js",
    "./types": "./dist/types/index.js",
    "./hooks": "./dist/hooks/index.js"
  },
  "typesVersions": {
    "*": {
      "utils": [
        "./dist/utils/index.d.ts"
      ],
      "types": [
        "./dist/types/index.d.ts"
      ],
      "hooks": [
        "./dist/hooks/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build:react": "tsc",
    "build:babel": "babel src --out-dir dist --extensions '.ts,.tsx,.js,.jsx'",
    "build": "npm run build:react && npm run build:babel",
    "prebuild": "rm -rf dist",
    "lint": "eslint",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{js,ts,json,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "glob": "^11.0.1",
    "react-i18next": "^15.4.0",
    "translate-projects-core": "^0.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/node": "^20.4.5",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "3.5.1",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT",
  "keywords": [
    "translate project",
    "translate projects react",
    "translate-projects docusaurus"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/translate-projects/translate-projects-react.git"
  },
  "bugs": {
    "url": "http://github.com/translate-projects/translate-projects-react/issues"
  },
  "author": {
    "name": "Neider Ruiz",
    "email": "neiderbtc@gmail.com",
    "url": "http://github.com/neiderruiz/"
  }
}
